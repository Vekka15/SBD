<%= form_for @new_player do |f|%>
  <%= f.label :nickname %>
  <%= f.text_field :nickname %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :surname %>
  <%= f.text_field :surname %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :team_name %>
  <%= f.text_field :team_name %>


  <%= f.submit %>

<%end%>
